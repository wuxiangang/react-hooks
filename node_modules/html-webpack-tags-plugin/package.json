    "debug": "node-debug jasmine"
  },
  "semistandard": {
    "ignore": [
      "spec/**/*-bundle.js",
      "spec/dist/**"
    ]
  },
  "unmockedModulePathPatterns": [
    "jasmine-expect"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jharris4/html-webpack-tags-plugin.git"
  },
  "keywords": [
