{
  "name": "html-webpack-tags-plugin",
  "version": "3.0.1",
  "description": "lets you define html tags to inject with html-webpack-plugin",
  "main": "index.js",
  "types": "typings.d.ts",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "index.js",
    "typings.d.ts"
  ],
  "scripts": {
    "prepublish": "npm run test",
    "pretest": "semistandard",
